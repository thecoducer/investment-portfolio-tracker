[run]
source = app
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */run_server/*
    */venv/*
    */env/*
    */.venv/*
    setup.py
    */site-packages/*
disable_warnings = module-not-imported

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
